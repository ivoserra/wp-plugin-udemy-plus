{
    "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
    "apiVersion": 2,
    "name": "udemy-plus/team-members-group",
    "title": "Team Members Group",
    "category": "text",
    "description": "Adds a group for adding team members",
    "textdomain": "udemy-plus",
    "attributes": {
      "columns": {
        "type": "number",
        "default": 3
      },
      "imageShape": {
        "type": "string",
        "default": "hexagon"
      }
    },
    "editorScript": "file:./index.js",
    "style": "file:./index.css",
    "providesContext": {
      "udemy-plus/image-shape": "imageShape"
    },
    "example": {
      "attributes": {
        "columns": 2
      },
      "innerBlocks": [
        {
          "name": "udemy-plus/team-member",
          "attributes": {
            "Name": "John Doe",
            "title": "CEO",
            "bio": "this is a short description of the team member",
            "imageUrl": "https://i.picsum.photos/id/237/100/100.jpg?hmac=Pna_vL4vYBRMXxFMY-lYXcZAL34T7PZWdNDlEOwqqE4"
            
          }
        },
        {
          "name": "udemy-plus/team-member",
          "attributes": {
            "Name": "Johanna Doe",
            "title": "CEO of CEO",
            "bio": "this is a short description of the team member",
            "imageUrl": "https://i.picsum.photos/id/237/100/100.jpg?hmac=Pna_vL4vYBRMXxFMY-lYXcZAL34T7PZWdNDlEOwqqE4"
          }
        }
      ]
    }
  }